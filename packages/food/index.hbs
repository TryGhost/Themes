{{!< default}}

<main class="gh-main">

    {{> "components/header"}}

    <div class="gh-inner">
        {{> "latest"}}

        {{#get "tags" include="count.posts" order="count.posts desc" limit="3"}}
            {{#foreach tags}}
                {{> "components/list"}}
            {{/foreach}}
        {{/get}}

        {{> "components/cta"}}

    </div>

</main>