<article class="{{post_class}}{{#unless feature_image}} no-image{{/unless}}">
  {{> post-media same_height=true ratio="rectangle"}}
  <div class="post-wrapper">
    {{> post-header}}
    {{#if excerpt}}
      <div class="post-excerpt">
        {{excerpt words="33"}}
      </div>
    {{/if}}
    {{> post-footer}}
  </div>
</article>